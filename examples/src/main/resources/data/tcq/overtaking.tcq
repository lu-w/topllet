# Overtaking

PREFIX phy: <http://purl.org/auto/physics#>

G (phy:Dynamical_Object(?x) ^ phy:Dynamical_Object(?y))
	&
F
(
	((phy:is_in_proximity(?x,?y) ^ phy:is_to_the_side_of(?y,?x)) | (phy:is_in_proximity(?x,?y) ^ phy:is_in_front_of(?y,?x)))
		&
	F (phy:is_in_proximity(?x,?y) ^ phy:is_behind(?y,?x))
)
